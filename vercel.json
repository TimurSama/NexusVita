{
  "buildCommand": "cd \"Nexus Vita\" && prisma generate && npm run build",
  "devCommand": "cd \"Nexus Vita\" && npm run dev",
  "installCommand": "cd \"Nexus Vita\" && npm install",
  "framework": "nextjs",
  "rootDirectory": "Nexus Vita",
  "regions": ["iad1"],
  "crons": [
    {
      "path": "/api/cron/sync",
      "schedule": "0 0 * * *"
    }
  ]
}
